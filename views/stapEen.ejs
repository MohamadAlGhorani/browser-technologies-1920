<%- include ./partials/head.ejs %>
<progress class="stappen" value="1" max="3">1/3</progress>
<section>
<h2>Inloggen met je student nummer</h2>
<p>Log in om het enquete in te vullen.</p>
<p>Als je het enquete niet afkrijgt, sla je enquete dan op om je enquete later af te krijgen</p>
<form class="form-stap-een" action="/enquete/stap-twee" method="GET">
	<label for="sNummer"> Student nummer:</label>
	<input id="sNummer" required="required" name="studentNummer" type="text" inputmode="numeric" pattern="[0-9]{9}*" autocomplete="one-time-code" minlength="9" maxlength="9" value="">
	<input  type="submit" value="Inloggen">
</form>
</section>
